extends layout

block content
	h1  櫃檯收入顯示 

	<br/>

				

	table.table
		tr  
			td  序號	
			td  起始日期
			td  結束日期
			td  收入
			td  備註
			
		- for (var i=0; i< data.length; i++){
			tr
				td 	#{i+1}
				td  #{data[i].start_date}
				td  #{data[i].end_date}
				td  #{data[i].revenue}
				td  #{data[i].remark}
		- }



